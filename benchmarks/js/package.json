{
    "name": "benchmarks-node",
    "version": "1.0.0",
    "description": "Benchmarks for master thesis",
    "main": "index.js",
    "author": "Piotr Tomczak",
    "license": "ISC",
    "scripts": {
        "bun:base64": "bun run ./bun/base64.js",
        "deno:base64": "deno run --allow-hrtime ./deno/base64.js",
        "node:base64": "node ./node/base64.js",
        "bun:sorting": "bun run ./bun/sorting.js",
        "deno:sorting": "deno run --allow-hrtime ./deno/sorting.js",
        "node:sorting": "node ./node/sorting.js",
        "node:createData": "node ./node/sqlite.js",
        "deno:createData": "deno run --allow-hrtime --allow-write --allow-read ./deno/sqlite.js",
        "bun:createData": "bun run ./bun/sqlite.js",
        "node:fileWrite": "node ./node/files.js",
        "deno:fileWrite": "deno run --allow-hrtime --allow-read --allow-write ./deno/files.js",
        "bun:fileWrite": "bun run ./bun/files.js",
        "node:server": "node ./node/server.js --watch",
        "deno:server": "deno run --allow-net --watch ./deno/server.js",
        "bun:server": "bun run ./bun/server.js"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.3.1"
    },
    "dependencies": {
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.6"
    }
}
