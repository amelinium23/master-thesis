{
	"name": "benchmarks-ts",
	"version": "1.0.0",
	"description": "Benchmarks for master thesis",
	"scripts": {
		"bun:base64": "bun run ./bun/base64.ts",
		"deno:base64": "deno run --allow-hrtime --allow-write --allow-read ./deno/base64.ts",
		"node:base64": "node ./node/base64.ts",
		"bun:sorting": "bun run ./bun/sorting.ts",
		"deno:sorting": "deno run --allow-hrtime --allow-write --allow-reade ./deno/sorting.ts",
		"node:sorting": "node ./node/sorting.ts",
		"node:createData": "node ./node/sqlite.ts",
		"deno:createData": "deno run --allow-hrtime --allow-write --allow-read ./deno/sqlite.ts",
		"bun:createData": "bun run ./bun/sqlite.ts",
		"node:fileWrite": "node ./node/files.ts",
		"deno:fileWrite": "deno run --allow-hrtime --allow-read --allow-write ./deno/files.ts",
		"bun:fileWrite": "bun run ./bun/files.ts",
		"node:server": "node ./node/server.ts --watch",
		"deno:server": "deno run --allow-net ./deno/server.ts",
		"bun:server": "bun run ./bun/server.ts"
	},
	"author": "Piotr Tomczak",
	"license": "ISC",
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@types/faker": "^6.6.9",
		"@types/node": "^20.10.5",
		"bun-types": "^1.0.9",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.6",
		"ts-node": "^10.9.1"
	}
}
